@model IEnumerable<MenuItem>

@if (Model.Count() > 0)
{
    <div class="col-12">
        <div class="row">
            @*this picks the very first item in the collection and displays its catgory name*@
            <h3 class="text-success"> @Model.FirstOrDefault().Category.Name </h3>
        </div>
    </div>
}
